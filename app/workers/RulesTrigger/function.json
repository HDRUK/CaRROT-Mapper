{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "route": "orchestrators/{functionName}",
      "methods": ["post", "get"]
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "msg",
      "queueName": "%RULES_QUEUE_NAME%",
      "connection": "STORAGE_CONN_STRING"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "starter",
      "type": "orchestrationClient",
      "direction": "in"
    }
  ]
}
